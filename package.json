{
    "name": "@michael-yakovlev/eslint-config",
    "version": "0.1.1",
    "description": "My personal ESLint config",
    "type": "module",
    "license": "MIT",
    "author": "Michael Yakovlev",
    "keywords": [
        "eslint",
        "eslint config",
        "eslint settings",
        "eslint configuration",
        "eslint javascript",
        "clean code",
        "code cleaner"
    ],
    "files": [
        "dist"
    ],
    "main": "./dist/index.mjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.mts",
    "exports": {
        ".": "./dist/index.mjs",
        "./package.json": "./package.json"
    },
    "engines": {
        "node": "^18.18.0 || ^20.9.0 || >= 21.1.0"
    },
    "scripts": {
        "build": "tsdown --sourcemap --minify",
        "dev": "tsdown --watch",
        "type-check": "tsc --noEmit",
        "lint": "eslint",
        "format": "prettier *.ts --write",
        "prepublishOnly": "pnpm run build"
    },
    "prettier": "@michael-yakovlev/prettier-config",
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kvadrofilii/eslint-config.git"
    },
    "packageManager": "pnpm@10.23.0",
    "dependencies": {
        "@eslint-react/eslint-plugin": "^2.3.7",
        "@eslint/js": "^9.39.1",
        "@eslint/json": "^0.14.0",
        "@tanstack/eslint-plugin-query": "^5.91.2",
        "@typescript-eslint/parser": "^8.47.0",
        "@vitest/eslint-plugin": "^1.4.3",
        "eslint": "^9.39.1",
        "eslint-config-prettier": "^10.1.8",
        "eslint-import-resolver-typescript": "^4.4.4",
        "eslint-plugin-import-x": "^4.16.1",
        "eslint-plugin-jsdoc": "^61.4.0",
        "eslint-plugin-jsonc": "^2.21.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-n": "^17.23.1",
        "eslint-plugin-playwright": "^2.3.0",
        "eslint-plugin-prettier": "^5.5.4",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-react-refresh": "^0.4.24",
        "eslint-plugin-regexp": "^2.10.0",
        "eslint-plugin-sonarjs": "^3.0.5",
        "eslint-plugin-unicorn": "^62.0.0",
        "globals": "^16.5.0",
        "jsonc-eslint-parser": "^2.4.1",
        "local-pkg": "^1.1.2",
        "typescript-eslint": "^8.47.0"
    },
    "devDependencies": {
        "@michael-yakovlev/prettier-config": "^1.0.7",
        "@types/eslint-plugin-jsx-a11y": "^6.10.1",
        "@types/node": "^24.10.1",
        "prettier": "^3.6.2",
        "tsdown": "^0.16.6",
        "typescript": "^5.9.3"
    },
    "peerDependencies": {
        "eslint": ">=9.39.1"
    },
    "overrides": {
        "eslint": "^9.39.1"
    }
}
