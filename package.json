{
    "publishConfig": {
        "access": "public"
    },
    "name": "@michael-yakovlev/eslint-config",
    "type": "module",
    "version": "0.0.6",
    "packageManager": "pnpm@10.15.0",
    "description": "My personal Eslint config",
    "author": "Michael Yakovlev",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kvadrofilii/eslint-config.git"
    },
    "keywords": [
        "eslint",
        "eslint config",
        "eslint settings",
        "eslint configuration",
        "eslint javascript",
        "eslint typescript",
        "clean code",
        "code cleaner"
    ],
    "exports": {
        "import": {
            "types": "./dist/index.d.mts",
            "default": "./dist/index.mjs"
        },
        "require": {
            "types": "./dist/index.d.cts",
            "default": "./dist/index.cjs"
        }
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.cts",
    "engines": {
        "node": "^18.18.0 || ^20.9.0 || >= 21.1.0"
    },
    "scripts": {
        "dev": "pkgroll --watch",
        "build": "pkgroll",
        "format": "prettier --write src",
        "type-check": "tsc --noEmit",
        "prepublishOnly": "pkgroll --clean-dist"
    },
    "prettier": "@michael-yakovlev/prettier-config",
    "dependencies": {
        "@eslint-react/eslint-plugin": "^1.52.6",
        "@eslint/css": "^0.10.0",
        "@eslint/js": "^9.34.0",
        "@eslint/json": "^0.13.1",
        "@eslint/markdown": "^7.2.0",
        "@next/eslint-plugin-next": "^15.5.0",
        "@tanstack/eslint-plugin-query": "^5.83.1",
        "@types/eslint-plugin-jsx-a11y": "^6.10.0",
        "@typescript-eslint/parser": "^8.40.0",
        "@typescript-eslint/utils": "^8.40.0",
        "@vitest/eslint-plugin": "^1.3.4",
        "eslint": "^9.34.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-import-resolver-typescript": "^4.4.4",
        "eslint-plugin-import-x": "^4.16.1",
        "eslint-plugin-jsdoc": "^54.1.1",
        "eslint-plugin-jsonc": "^2.20.1",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-n": "^17.21.3",
        "eslint-plugin-prettier": "^5.5.4",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.20",
        "eslint-plugin-regexp": "^2.10.0",
        "eslint-plugin-unicorn": "^60.0.0",
        "eslint-plugin-vue": "^10.4.0",
        "eslint-plugin-vuejs-accessibility": "^2.4.1",
        "globals": "^16.3.0",
        "jsonc-eslint-parser": "^2.4.0",
        "local-pkg": "^1.1.2",
        "typescript-eslint": "^8.40.0"
    },
    "devDependencies": {
        "@michael-yakovlev/prettier-config": "^1.0.6",
        "@types/node": "^24.3.0",
        "pkgroll": "^2.15.3",
        "prettier": "^3.6.2",
        "typescript": "^5.9.2"
    },
    "peerDependencies": {
        "eslint": "^9.34.0"
    },
    "overrides": {
        "eslint": "^9.34.0"
    }
}
